{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Adresse",
    "type": "object",
    "_meta": {
        "type": "location",
        "link": "locations",
        "domain": "locations"
    },
    "properties": {
        "id": {
            "type": "string",
            "options": {
                "hidden": true
            }
        },
        "display_label": {
            "title": "Primær label",
            "type": "string",
            "_meta": {
                "type": "string"
            },
            "minLength": 1,
            "propertyOrder": 1
        },
        "name": {
            "title": "Vejnavn",
            "type": "string",
            "_meta": {
                "type": "string"
            },
            "minLength": 1,
            "propertyOrder": 2
        },
        "addr_nr": {
            "title": "Husnummer",
            "type": "string",
            "_meta": {
                "type": "string"
            },
            "minLength": 1,
            "propertyOrder": 3
        },
        "alt_names": {
            "title": "Alternative navne",
            "type": "array",
            "items": {
                "type": "string"
            },
            "_meta": {
                "type": "string_list",
                "link": "locations"
            },
            "propertyOrder": 4
        },
        "date_from": {
            "title": "Oprettelsesdato (åååå-mm-dd)",
            "type": "string",
            "_meta": {
                "type": "string"
            },
            "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$|^$",
            "default": "",
            "propertyOrder": 5
        },
        "date_to": {
            "title": "Nedlæggelsesdato (åååå-mm-dd)",
            "type": "string",
            "_meta": {
                "type": "string"
            },
            "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}$|^$",
            "propertyOrder": 6
        },
        "description": {
            "title": "Beskrivelse (adskil hvert afsnit med en tom linje)",
            "type": "string",
            "_meta": {
                "type": "paragraphs"
            },
            "format": "textarea",
            "propertyOrder": 7
        },
        "parish": {
            "type": "string",
            "title": "Sogn",
            "_meta": {
                "type": "string"
            },
            "propertyOrder": 8
        },
        "zipcode": {
            "title": "Postnummer",
            "type": "string",
            "pattern": "^[0-9]{4}$|^$",
            "_meta": {
                "type": "string"
            },
            "propertyOrder": 9
        },
        "coordinates": {
            "title": "Koordinater",
            "properties": {
                "longitude": {
                    "title": "Længdegrad",
                    "type": [
                        "null",
                        "number"
                    ],
                    "minimum": -180,
                    "maximum": 180
                },
                "latitude": {
                    "title": "Breddegrad",
                    "type": [
                        "null",
                        "number"
                    ],
                    "minimum": -90,
                    "maximum": 90
                }
            },
            "_meta": {
                "type": "latitude_longitude_normalized"
            },
            "propertyOrder": 10
        },
        "rotation": {
            "title": "Rotation",
            "type": [
                "null",
                "number"
            ],
            "_meta": {
                "type": "string"
            },
            "minimum": 0,
            "maximum": 360,
            "propertyOrder": 11
        },
        "local_area": {
            "title": "Lokalområde",
            "type": "string",
            "_meta": {
                "type": "string"
            },
            "propertyOrder": 12
        },
        "portrait": {
            "title": "Billeder (kan benyttes til slideshows)",
            "type": "array",
            "items": {
                "type": "string",
                "format": "uri",
                "pattern": "^https://.*",
                "default": ""
            },
            "_meta": {
                "type": "image_list"
            },
            "propertyOrder": 13
        },
        "sources_links": {
            "title": "Kilder (som links)",
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "label": {
                        "title": "Label",
                        "type": "string",
                        "minLength": 1
                    },
                    "url": {
                        "title": "URL",
                        "type": "string",
                        "format": "uri",
                        "minLength": 1,
                        "pattern": "^https://.*"
                    }
                },
                "required": [
                    "label",
                    "url"
                ]
            },
            "_meta": {
                "type": "url_list"
            },
            "propertyOrder": 14
        },
        "sources_texts": {
            "title": "Kilder (som tekst)",
            "type": "array",
            "items": {
                "type": "string"
            },
            "_meta": {
                "type": "string_list"
            },
            "propertyOrder": 15
        },
        "ext_data": {
            "title": "Udvidet data",
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "key": {
                        "title": "Nøgle",
                        "type": "string",
                        "minLength": 1

                    },
                    "value": {
                        "title": "Værdi",
                        "type": "string",
                        "minLength": 1
                    }
                },
                "required": [
                    "key",
                    "value"
                ]
            },
            "_meta": {
                "type": "key_value_dict"
            },
            "propertyOrder": 16
        }
    },
    "required": [
        "display_label",
        "name",
        "addr_nr"
    ]
}